#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("Insira uma string: \n");
    char str[100];
    fgets(str,100,stdin);
    int count = 0,i;
    while (str[i]!='\0'){
        i++;
        count = count + 1;
    }
    
    printf("O comprimento da string e %d\n",count);
    return 0;
}
