#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("Insira uma string de numeros: \n");
    char str[100];
    fgets(str,100,stdin);
    int count = 0;
    int i;
    for (i = 0; i < 100; i++){
        if (str[i]=='0'){
            str[i]='1';
        }
    }
    
    printf("%s\n",str); //substituindo 0s por 1s
    return 0;
}
