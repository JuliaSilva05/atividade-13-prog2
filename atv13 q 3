#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("Insira uma string: \n");
    char str[100];
    fgets(str,100,stdin);
    int count1 = 0;
    int count2 = 0;
    for (int i = 0; i < 100; i++){
        if (str[i]!='\0'){
            count1 = count1 + 1;
        }
    }
    printf("Insira outra string: \n");
    char str2[100];
    fgets(str2,100,stdin);
    for (int j = 0; j < 100; j++){
        if (str2[j]!='\0'){
            count2 = count2 + 1;
        }
    }
    if (count1 == count2){
        printf("As strings possuem o mesmo comprimento\n");
    } else{
        printf("As strings possuem comprimentos diferentes\n");
    }
    return 0;
}
