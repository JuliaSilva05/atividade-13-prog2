#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    printf("Insira uma string: \n");
    char str[100];
    char strDois[100];
    fgets(str,100,stdin);
    int count = strlen(str);
    printf("Insira outra string\n");
    fgets(strDois,100,stdin);
    int countDois = strlen(strDois);
    if (count == countDois){
        printf("As strings possuem o mesmo comprimento\n");
    } else{
        printf("As strings possuem comprimentos diferentes\n");
    }
    
    return 0;
}
